<div class='center'>
<h3>DIAGRAM KOMPETENSI</H3>

 <div class="radarChart" style=" background-image: url('/images/1/27.png'); 
 background-position: center; background-repeat: no-repeat;background-size: 400px 420px;"></div>
 <script src="/js/radarChart.js"></script>

 
		<script>
      
      /* Radar chart design created by Nadieh Bremer - VisualCinnamon.com */
      
			////////////////////////////////////////////////////////////// 
			//////////////////////// Set-Up ////////////////////////////// 
			////////////////////////////////////////////////////////////// 

			var margin = {top: 10, right: 50, bottom: 35, left: 50},
				width = Math.min(500, window.innerWidth - 10) - margin.left - margin.right,
				height = Math.min(width, window.innerHeight - margin.top - margin.bottom - 20);
					
			////////////////////////////////////////////////////////////// 
			////////////////////////// Data ////////////////////////////// 
			////////////////////////////////////////////////////////////// 
			var data = [
				[
			{axis:'',value:'4'},{axis:'',value:'4'},{axis:'',value:'3'},{axis:'',value:'4'},{axis:'',value:'4'},{axis:'',value:'3'},{axis:'',value:'4'},{axis:'',value:'3'},{axis:'',value:'4'},{axis:'',value:'3'},				],[
					{axis:'',value:'3'},{axis:'',value:'3'},{axis:'',value:'2'},{axis:'',value:'3'},{axis:'',value:'3'},{axis:'',value:'2'},{axis:'',value:'3'},{axis:'',value:'2'},{axis:'',value:'3'},{axis:'',value:'3'},				]
			  ];

			////////////////////////////////////////////////////////////// 
			//////////////////// Draw the Chart ////////////////////////// 
			////////////////////////////////////////////////////////////// 

			var color = d3.scale.ordinal()
			.range(['#AEDFFB','#35274E']);
			
			var radarChartOptions = {
			w: width,
			h: height,
			margin: margin,
			maxValue: 6,
			levels: 5,
			roundStrokes: false,
			color: color,
			opacityArea: 0.5,
			opacityCircles: 0,
			dotRadius: 3,
			strokeWidth: 2, 
			wrapWidth: 10,
			labelFactor: 10,
			};
			//Call function to draw the Radar chart
			RadarChart(".radarChart", data, radarChartOptions);
		</script>
</div>


